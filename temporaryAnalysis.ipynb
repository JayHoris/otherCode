   "source": [
    "from sympy import symbols, limit, oo\n",
    "\n",
    "# 定义符号变量\n",
    "k = symbols('k')\n",
    "\n",
    "\n",
    "\n",
    "# 构造表达式\n",
    "expression = ((1+1/2**(k+1))**2-1) / ((1 + 1/2**k)**2 -1)**2\n",
    "\n",
    "# 计算极限\n",
    "lim_result = limit(expression, k, oo)\n",
    "lim_result"
   ]
  }
